{% extends '@admin/layout.twig' %}

{% block title "Éditer la catégorie" %}

{% block body %}

    <h1>Éditer la catégorie : {{ item.name }}</h1>

    <form action="{{ path(routePrefix ~ '.edit', {id: item.id}) }}" method="post">
        {% include viewPath ~ '/form.twig' %}
        <button class="btn btn-primary mt-2" type="submit">Éditer</button>
    </form>

{% endblock %}
